    },
    "keywords": [
        "tao",
        "oat",
        "QTI",
        "computer-based-assessment"
    ],
    "homepage": "http://www.taotesting.com",
    "license": [
        "GPL-2.0-only"
    ],
    "extra": {
        "tao-extension-name": "taoQtiItem",
        "npm-bridge": {
            "arguments": [
                "--prefix",
                "views"
            ]
        }
    },
    "minimum-stability": "dev",
   "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:openthinklabs/oatbox-extension-installer.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:openthinklabs/generis.git"
        },        
        {
            "type": "vcs",
            "url": "git@github.com:openthinklabs/tao-core.git"
        },                
        {
            "type": "vcs",
            "url": "git@github.com:openthinklabs/extension-tao-backoffice.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:openthinklabs/lib-generis-search.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:openthinklabs/jig.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:openthinklabs/lib-tao-qti.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:openthinklabs/extension-tao-item.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:openthinklabs/qti-sdk.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:openthinklabs/composer-npm-bridge.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:openthinklabs/lib-beeme.git"
        }                                                        
    ],	     
    "require": {
        "openthinklabs/lib-tao-qti": "7.3.1.3",
        "openthinklabs/oatbox-extension-installer": "1.2.2",
        "naneau/semver": "~0.0.7",
        "openthinklabs/generis" : "15.11.0.4",
        "openthinklabs/tao-core" : "~48.66.1.4",
        "openthinklabs/extension-tao-item" : "~11.21.0.5"
    },
    "autoload": {
        "psr-4": {
            "oat\\taoQtiItem\\": ""
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "openthinklabs/oatbox-extension-installer": true,
            "openthinklabs/composer-npm-bridge": true
        }
    }
}
